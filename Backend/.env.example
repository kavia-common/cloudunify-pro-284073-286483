# PostgreSQL connection settings (do not commit real credentials)
PGHOST=localhost
PGPORT=5432
PGUSER=postgres
PGPASSWORD=postgres
PGDATABASE=cloudunify_pro
# Optional
PGSSL=false
PGPOOL_MAX=10
# Alternatively, you can set:
# DATABASE_URL=postgresql://user:password@host:5432/dbname

# Optional override for where to find .projdefn files
# If not set, the app will attempt to locate a .projdefn directory by walking up the tree.
PROJDEFN_DIR=

# Seed admin token required in production when calling /_internal/seed endpoints
SEED_ADMIN_TOKEN=change-me-strong-token
